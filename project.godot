; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "",
"class": "FLMusicLib",
"language": "NativeScript",
"path": "res://FLMusicLib/FLMusic.gdns"
}, {
"base": "",
"class": "GitAPI",
"language": "NativeScript",
"path": "res://git_api.gdns"
}, {
"base": "Reference",
"class": "GodotHub",
"language": "GDScript",
"path": "res://godothub/scripts/godothub.gd"
} ]
_global_script_class_icons={
"FLMusicLib": "",
"GitAPI": "",
"GodotHub": ""
}

[application]

config/name="Sonadow RPG"
run/main_scene="res://Scenes/ServerAPI/updater.tscn"
boot_splash/image="res://Graphics/bootlogo.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/bg_color=Color( 1, 1, 1, 1 )
config/icon="res://Graphics/icon.png"
config/windows_native_icon="res://Graphics/icon.ico"

[autoload]

Screen="*res://addons/screen_debugger/screen.scn"
Globals="*res://Scripts/Globals.gd"
Modloader="*res://Scripts/modloader.gd"
BackgroundLoad="*res://Scenes/background_load.tscn"
Console="*res://addons/godot-console-dev/src/Console.tscn"
ScreenDebugger="*res://addons/screen_debugger/S_ScreenDebugger.gd"
API="*res://Scenes/ServerAPI/API.tscn"

[debug]

settings/crash_handler/message="Please include this when reporting the bug on https://github.com/MasterPolska123/sonadow-rpg/issues/"

[display]

window/dpi/allow_hidpi=true
window/vsync/vsync_via_compositor=true

[editor]

convert_text_resources_to_binary_on_export=true

[editor_plugins]

enabled=PoolStringArray( "godot-console-dev", "godot-kra-psd-importer", "godot-sqlite", "screen_debugger" )

[gdnative]

singletons=[ "res://git_api.gdnlib" ]

