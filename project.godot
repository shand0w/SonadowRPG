; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Area2D",
"class": "Crate",
"language": "GDScript",
"path": "res://minimap/Crate.gd"
}, {
"base": "",
"class": "Discord",
"language": "NativeScript",
"path": "res://libdiscord.gdns"
}, {
"base": "KinematicBody2D",
"class": "Mob",
"language": "GDScript",
"path": "res://minimap/Mob.gd"
}, {
"base": "KinematicBody2D",
"class": "Player",
"language": "GDScript",
"path": "res://minimap/Player.gd"
} ]
_global_script_class_icons={
"Crate": "",
"Discord": "",
"Mob": "",
"Player": ""
}

[application]

config/name="Sonadow RPG"
run/main_scene="res://Scenes/ServerAPI/updater.tscn"
config/project_settings_override="res://rakugo.cfg"
boot_splash/image="res://Graphics/bootlogo.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/bg_color=Color( 1, 1, 1, 1 )
config/icon="res://Graphics/icon.png"
config/windows_native_icon="res://Graphics/icon.ico"
rakugo/version="0.0.1"
rakugo/game_credits="Your Company"
rakugo/markup="renpy"
rakugo/debug=true
rakugo/test_saves=false
rakugo/scenes_links="res://game/scenes_links.tres"
rakugo/theme="res://themes/question/question.tres"
rakugo/default_kind="nvl"
rakugo/punctuation_pause="1"
rakugo/default_mkind="grid"
rakugo/default_mcolumns=2.0
rakugo/default_manchor="center"
rakugo/scene_links="res://game/scene_links.tres"

[autoload]

Screen="*res://addons/screen_debugger/screen.scn"
Globals="*res://Scripts/Globals.gd"
BackgroundLoad="*res://Scenes/background_load.tscn"
Console="*res://addons/godot-console-dev/src/Console.tscn"
ScreenDebugger="*res://addons/screen_debugger/S_ScreenDebugger.gd"
API="*res://Scenes/ServerAPI/API.tscn"
Rakugo="*res://addons/Rakugo/Rakugo.tscn"

[debug]

settings/crash_handler/message="Please include this when reporting the bug on https://github.com/MasterPolska123/sonadow-rpg/issues/"

[display]

window/dpi/allow_hidpi=true
window/vsync/vsync_via_compositor=true

[editor]

convert_text_resources_to_binary_on_export=true

[editor_plugins]

enabled=PoolStringArray( "godot-console-dev", "screen_debugger" )

[gdnative]

singletons=[ "res://videodecoder.gdnlib" ]

